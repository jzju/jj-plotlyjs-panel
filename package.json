{
  "name": "nline-plotlyjs-panel",
  "version": "1.8.0",
  "description": "Grafana Plotly Plugin",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "docker": "docker run -d -p 3000:3000 -v \"$(pwd)\":/var/lib/grafana/plugins --name=grafana grafana/grafana",
    "e2e": "yarn exec cypress install && yarn exec grafana-e2e run",
    "e2e:update": "yarn exec cypress install && yarn exec grafana-e2e run --update-screenshots",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn run lint --fix",
    "md5": "md5 nline-plotlyjs-panel-%npm_package_version%.zip nline-plotlyjs-panel-%npm_package_version%.zip.md5",
    "package": "./package.sh",
    "release": "tag=$(git describe --abbrev=0) && gh release create ${tag} ./nline-plotlyjs-panel-${tag}.zip ./nline-plotlyjs-panel-${tag}.zip.md5",
    "upload": "tag=$(git describe --abbrev=0) && gh release upload ${tag} ./nline-plotlyjs-panel-${tag}.zip ./nline-plotlyjs-panel-${tag}.zip.md5 --clobber",
    "server": "docker-compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "validate": "tag=$(git describe --abbrev=0) && npx -y @grafana/plugin-validator@latest -sourceCodeUri https://github.com/nline/nline-plotlyjs-panel/tree/${tag} https://github.com/nline/nline-plotlyjs-panel/releases/download/${tag}/nline-plotlyjs-panel-${tag}.zip"
  },
  "author": "nLine",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@grafana/e2e": "^11.0.2",
    "@grafana/e2e-selectors": "^11.1.3",
    "@grafana/eslint-config": "^7.0.0",
    "@grafana/tsconfig": "^1.3.0-rc1",
    "@swc/core": "^1.7.6",
    "@swc/helpers": "^0.5.12",
    "@swc/jest": "^0.2.36",
    "@testing-library/jest-dom": "6.4.8",
    "@testing-library/react": "16.0.0",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.7",
    "@types/node": "^22.1.0",
    "@types/plotly.js-dist-min": "^2.3.4",
    "@types/react-plotly.js": "^2.6.3",
    "@types/react-router-dom": "^5.3.3",
    "@types/testing-library__jest-dom": "6.0.0",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "glob": "^11.0.0",
    "identity-obj-proxy": "3.0.0",
    "imports-loader": "^5.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.3.3",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.77.8",
    "sass-loader": "16.0.0",
    "style-loader": "4.0.0",
    "swc-loader": "^0.2.6",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.5.4",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2",
    "webpack-virtual-modules": "^0.6.2"
  },
  "dependencies": {
    "@emotion/css": "^11.13.0",
    "@grafana/data": "^11.1.3",
    "@grafana/runtime": "^11.1.3",
    "@grafana/schema": "^11.1.3",
    "@grafana/ui": "^11.1.3",
    "@types/file-saver": "^2.0.7",
    "@types/plotly.js": "^2.33.3",
    "dayjs": "^1.11.12",
    "deepmerge": "^4.3.1",
    "file-saver": "^2.0.5",
    "js-yaml": "^4.1.0",
    "plotly.js-dist-min": "^2.34.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-plotly.js": "^2.6.0",
    "react-resizable": "^3.0.5",
    "tslib": "2.6.3"
  },
  "packageManager": "yarn@1.22.22"
}
